user  {{ user }};
worker_processes  1;

error_log  /var/log/nginx/error.log warn;
pid        /var/run/nginx.pid;


events {
    worker_connections  1024;
}


http {
    upstream apachepool {
        server 127.0.0.1:81;
        server 127.0.0.1:8008;
    }

    server {
        listen {{ nginx.port }};

        location / {
            proxy_pass http://apachepool;
        }
    }
}